<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout
                pattern="%d{yyyy-MM-dd HH:mm:ss,SSS} %-5p %c{3}.%M(%F:%L) - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="ALL" additivity="false">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>